pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World';
            }
        }
        
        stage('Junit'){
            steps {
                echo "Junit passed successfully!";
            }
        }
        
        stage('Last Step'){
            steps {
                echo "Last step passed successfully!";
            }
        }
        
        
    }
}
